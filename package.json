{
  "name": "@thinkeloquent/babel-extract-imports-text-based-notation",
  "version": "0.0.5",
  "description": "Extract imports from JavaScript/TypeScript code in a text-based notation format",
  "main": "index.mjs",
  "type": "module",
  "private": false,
  "scripts": {
    "test": "npx vitest",
    "commit": "cz",
    "commit:general": "git commit -m \"ci: general update\"",
    "prepublishOnly": "npx vitest run --coverage",
    "prepare": "husky"
  },
  "keywords": [
    "babel",
    "imports",
    "parser",
    "typescript",
    "javascript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.x",
    "@babel/traverse": "^7.x"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@vitest/coverage-v8": "^3.2.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "vitest": "^3.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
